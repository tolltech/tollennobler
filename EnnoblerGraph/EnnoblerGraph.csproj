<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net48</TargetFramework>
    <AssemblyName>Tolltech.EnnoblerGraph</AssemblyName>
    <RootNamespace>Tolltech.EnnoblerGraph</RootNamespace>
    <LangVersion>8</LangVersion>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Ennobler\Ennobler.csproj" />
  </ItemGroup>

  <ItemGroup>
    <None Update="codeMetrics.xml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="input.csv">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="run.bat">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
    <PackageReference Include="Vostok.Logging.Console" Version="1.0.5" />
    <PackageReference Include="Vostok.Logging.File" Version="1.0.12" />
  </ItemGroup>

</Project>
